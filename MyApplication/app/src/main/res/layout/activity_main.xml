<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a8732214.myapplication.MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A few word about SAMROBOT"
        android:textColor="@android:color/black"
        android:textSize="14sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="346dp"
        android:layout_height="132dp"
        android:text="School presentation : \nESIGELEC is an engineering School"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="55dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="346dp"
        android:layout_height="132dp"
        android:text="Group presentation : \nDONNET Marie, LATOUR Rodolphe and PHAN TRAN Toan composed this group"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="210dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="350dp"
        android:layout_height="131dp"
        android:text="Robot presentation : \nSamRobot is a autonome or manual\nRobot."
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="364dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="139dp"
        android:layout_height="36dp"
        app:srcCompat="@drawable/logo_esigelec"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="55dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="161dp"
        android:layout_height="261dp"
        app:srcCompat="@drawable/robot_PNG79"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="234dp" />

</android.support.constraint.ConstraintLayout>
